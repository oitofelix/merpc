<form>
  <h1 style="width: 100%">RDV: Relatório de Despesas de Viagens</h1>
  <label style="width: 50%">Veículo<input id="veiculo" type="text" /></label>
  <label style="width: 50%">Placa<input id="placa" type="text" /></label>
  <label style="width: 50%">Início<input type="date" /></label>
  <label style="width: 50%">Fim<input type="date"></label>
  <label style="width: 100%">
    <select>
      <option>Hospedagem</option>
      <option>Veículos</option>
      <option>Alimentação</option>
    </select>
  </label>
  <label style="width: 100%">Data<input type="date" /></label>
  <label style="width: 100%">NF/Cupom<input type="number" /></label>
  <label style="width: 50%">Qtd<input type="number" /></label>
  <label style="width: 50%">Valor<input type="number" /></label>
  <label style="width: 100%">
    <textarea placeholder="Obs."></textarea>
  </label>
  <label style="width: 100%">
    <select>
      <option>Fundo Fixo Mensal</option>
      <option>Cartão Corporativo</option>
      <option>Ajuda de Custo de Veículo</option>
      <option>Valor Fixo Mensal</option>
    </select>
  </label>
  <label style="width: 20%">
    <input type="button" value="Enviar" />
  </label>
  <label id="exit" class="r" style="width: 20%">
    <input type="button" value="Sair" />
  </label>
</form>
